apiVersion: v1
kind: Config
clusters:
  - cluster:
      server: ${apiserver_endpoint}
      certificate-authority: ${cacert_file}
    name: ${cluster_name}
contexts:
  - context:
      cluster: ${cluster_name}
      user: ${username}
    name: default
users:
  - name: ${username}
    user:
      token: ${token}
current-context: default
